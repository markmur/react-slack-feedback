{
  "name": "react-slack-feedback",
  "version": "2.0.2",
  "description": "React component for gathering user feedback to send to slack.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "author": "Mark Murray <mark.murray.ie@gmail.com> (https://github.com/markmur)",
  "license": "MIT",
  "scripts": {
    "build": " NODE_ENV=production rollup --config rollup.config.js",
    "bundlesize": "bundlesize",
    "lint-staged": "lint-staged",
    "lint": "xo",
    "start:client": "NODE_ENV=development parcel --no-cache demo/index.html",
    "start:server": "nodemon demo/server --watch demo/server.js",
    "start": "yarn start:server & yarn start:client",
    "test:ci": "jest && codecov -t 418f17d1-6515-4c5c-9707-3d49c409cf20",
    "test": "jest",
    "prepublishOnly": "rimraf dist && yarn build"
  },
  "bundlesize": [
    {
      "path": "dist/index*.js",
      "maxSize": "7 kB"
    }
  ],
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "react": "^0.14 || >=15",
    "styled-components": ">=4"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --single-quote --write",
      "git add",
      "xo"
    ]
  },
  "precommit": {
    "run": [
      "lint-staged",
      "test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markmur/react-slack-feedback"
  },
  "keywords": [
    "react",
    "slack",
    "feedback"
  ],
  "bugs": {
    "url": "https://github.com/markmur/react-slack-feedback/issues"
  },
  "homepage": "https://github.com/markmur/react-slack-feedback",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "body-parser": "^1.15.2",
    "bundlesize": "^0.17.1",
    "codecov": "^3.0.0",
    "cors": "^2.8.5",
    "cssnano": "^3.7.3",
    "danger": "^3.2.0",
    "dotenv": "^6.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-react": "^0.18.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.14.0",
    "foreman": "^2.0.0",
    "fs-extra": "^7.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "multer": "^1.2.0",
    "nodemon": "^1.18.10",
    "parcel-bundler": "^1.11.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.2",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.2.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "styled-components": "^4.1.3",
    "xo": "^0.24.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.js"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost:8080",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true
  },
  "xo": {
    "parser": "babel-eslint",
    "envs": [
      "browser",
      "node",
      "jest/globals"
    ],
    "prettier": true,
    "space": true,
    "rules": {
      "camelcase": 0,
      "import/prefer-default-export": 0
    },
    "extends": "xo-react",
    "plugins": [
      "react",
      "jest"
    ]
  }
}
